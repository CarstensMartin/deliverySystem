package foodQuick;

public class Driver {
	// Attributes
	String driverName;
	String cityDriver;
	String deliveries;

	// Methods to declare object construction
	public Driver(String driverName, String cityDriver, String deliveries) {
		this.driverName = driverName;
		this.cityDriver = cityDriver;
		this.deliveries = deliveries;
	}

}
